{
  "name": "mathematico",
  "version": "2.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "dev": "expo start --dev-client --clear",
    "dev:go": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev": "eas build --platform android --profile development",
    "build:android": "eas build --platform android --profile production",
    "build:android-preview": "eas build --platform android --profile preview",
    "build:ios": "eas build --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.3.1",
    "@react-navigation/stack": "^7.6.13",
    "axios": "^1.13.2",
    "expo": "~54.0.0",
    "expo-constants": "~18.0.0",
    "expo-dev-client": "~6.0.0",
    "expo-document-picker": "~14.0.0",
    "expo-file-system": "~19.0.0",
    "expo-font": "~14.0.10",
    "expo-image-picker": "~17.0.0",
    "expo-linear-gradient": "~15.0.0",
    "expo-secure-store": "~15.0.0",
    "expo-splash-screen": "~31.0.0",
    "expo-status-bar": "~3.0.0",
    "expo-updates": "~29.0.0",
    "fs-extra": "^11.3.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-paper": "^5.14.5",
    "react-native-razorpay": "^2.2.14",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.7.2",
    "react-refresh": "^0.14.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.30",
    "@types/react": "~19.1.2",
    "expo-build-properties": "~1.0.10",
    "jest-expo": "~54.0.16",
    "patch-package": "^8.0.1",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=20.19.4",
    "npm": ">=10.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": false
      },
      "configSchemaCheck": {
        "enabled": false
      }
    },
    "install": {
      "exclude": [
        "expo",
        "expo-constants",
        "expo-updates",
        "react-native-worklets",
        "expo-font"
      ]
    }
  },
  "description": "Mathematico mobile app powered by Expo SDK 54",
  "keywords": [
    "mathematico",
    "expo",
    "react-native"
  ],
  "author": "",
  "license": "MIT"
}
