{
  "version": 2,
  "builds": [
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/Backend/index.js" }
  ],
  "functions": {
    "Backend/index.js": {
      "runtime": "@vercel/node",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
