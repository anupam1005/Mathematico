{
  "version": 2,
  "builds": [
    {
      "src": "Backend/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30,
        "memory": 1024
      }
    }
  ],
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/Backend/index.js"
    },
    {
      "src": "/robots.txt",
      "dest": "/Backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Backend/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1",
    "BACKEND_URL": "https://mathematico-backend-new.vercel.app",
    "FRONTEND_URL": "https://mathematico-frontend.vercel.app",
    "MONGODB_URI": "mongodb+srv://smartfarm:mathematico@mathematico.sod8pgp.mongodb.net/",
    "MONGODB_DB": "mathematico",
    "API_PREFIX": "/api/v1",
    "RATE_LIMIT_WINDOW_MS": "900000",
    "RATE_LIMIT_MAX_REQUESTS": "100"
  },
  "regions": ["iad1"]
}