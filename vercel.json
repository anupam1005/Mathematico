{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": "/logo.png",
        "dest": "/logo.png"
      },
      {
        "src": "/placeholder.svg",
        "dest": "/placeholder.svg"
      },
      {
        "src": "/robots.txt",
        "dest": "/robots.txt"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "VITE_API_URL": "https://YOUR-ACTUAL-BACKEND-URL.herokuapp.com/api/v1",
      "VITE_BACKEND_URL": "https://YOUR-ACTUAL-BACKEND-URL.herokuapp.com",
      "VITE_APP_NAME": "Mathematico",
      "VITE_ENABLE_DEBUG_MODE": "false"
    }
  }