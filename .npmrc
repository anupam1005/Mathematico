# Force Windows platform only
target_platform=win32
target_arch=x64
target_libc=undefined

# Exclude Linux packages
platform=win32
arch=x64

# Ignore optional dependencies that might cause platform issues
optional=false

# Use npm cache
cache-min=86400

# Don't install dev dependencies in production
production=false

# Platform-specific package resolution
engine-strict=true

# Exclude problematic packages
fund=false
audit=false
